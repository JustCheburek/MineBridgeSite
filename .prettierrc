{
  "semi": false,
  "singleQuote": true,
  "tabWidth": 2,
  "printWidth": 100,
  "trailingComma": "es5",
  "arrowParens": "avoid",
  "jsxSingleQuote": true,
  "bracketSpacing": true,
  "plugins": ["prettier-plugin-tailwindcss"],
  "tailwindStylesheet": "./app/global.css",
  "tailwindAttributes": ["className"],
  "tailwindFunctions": ["clsx", "cn"],
  "tailwindPreserveWhitespace": true
}
